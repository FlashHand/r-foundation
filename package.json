{
  "name": "r-foundation",
  "version": "1.2.0",
  "description": "",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:esm": "esbuild src/index.ts --bundle --platform=node --format=esm --outdir=esm --external:dayjs --external:lodash && tsc --emitDeclarationOnly --declaration --outDir esm",
    "build:cjs": "esbuild src/index.ts --bundle --platform=node  --outdir=cjs --external:dayjs --external:lodash && tsc --emitDeclarationOnly --declaration --outDir cjs",
    "build": "yarn build:esm && yarn build:cjs"
  },
  "exports":{
    ".": {
      "import": "./esm/index.js",
      "require": ".cjs/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git@code.aliyun.com:rys-fe-modules/r-foundation.git"
  },
  "author": "王博",
  "license": "ISC",
  "dependencies": {
    "dayjs": "^1.8.30",
    "lodash": "^4.17.19"
  },
  "devDependencies": {
    "@types/node": "^18.15.3",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  }
}
